#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'cv-ros2==0.0.0','console_scripts','pub_node=cv_ros2.img_pub:mainsub_node'
__requires__ = 'cv-ros2==0.0.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('cv-ros2==0.0.0', 'console_scripts', 'pub_node=cv_ros2.img_pub:mainsub_node')()
    )
